<h1>Sign up</h1>
<%= form_with(model: @user, local: true) do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <%= f.label :user_email %>
  <%= f.email_field :user_email %>
  <%= f.label :パスワード %>
  <%= f.password_field :password %>
  <%= f.label :パスワード確認 %>
  <%= f.password_field :password_confirmation %>
  <%= f.submit "新規登録" %>
<% end %>
